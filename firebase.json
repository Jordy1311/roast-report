{
  "hosting": {
    "site": "roastreport",
    "public": "dist/browser/",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "**",
        "headers": [{
          "key": "Cache-Control",
          "value": "public, max-age=2592000"
        }]
      }
    ]
  }
}
