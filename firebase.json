{
  "hosting": {
    "site": "roastreport",
    "public": "/dist/browser",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "hosting": {
      "public": "dist/browser",
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  }
}
